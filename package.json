{
  "name": "rbi-system-frontend",
  "version": "0.1.0",
  "description": "RBI System - Records of Barangay Inhabitant System Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/prepare-storybook.js && npm run build-storybook",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}' '*.{js,jsx,ts,tsx,json,md}' '.storybook/**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}' '*.{js,jsx,ts,tsx,json,md}' '.storybook/**/*.{js,jsx,ts,tsx,json}'",
    "cleanup": "npm run format && npm run lint:fix",
    "pre-push": "npm run type-check && npm run lint && npm run test:ci",
    "security:check": "./scripts/security-check.sh",
    "security:audit": "npm audit --production",
    "security:audit-fix": "npm audit fix --production",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@hookform/resolvers": "^3.3.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^4.36.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.47.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/nextjs-vite": "^9.1.1",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.31",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-storybook": "^9.1.1",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.1.4",
    "playwright": "^1.54.2",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "storybook": "^9.1.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
