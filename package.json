{
  "name": "rbi-system-frontend",
  "version": "0.2.0",
  "description": "RBI System - Records of Barangay Inhabitant System Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --hostname 0.0.0.0 --port 3000",
    "dev:staging": "next dev --env .env.staging",
    "dev:production": "next dev --env .env.production",
    "build": "npm run build:check-branch && node scripts/build-env.mjs && next build",
    "build:check-branch": "echo \"Build check: Branch is $VERCEL_GIT_COMMIT_REF\"; if [ -n \"$VERCEL_GIT_COMMIT_REF\" ]; then case \"$VERCEL_GIT_COMMIT_REF\" in main|staging|develop) echo \"Proceeding with build for target branch: $VERCEL_GIT_COMMIT_REF\";; *) echo \"Skipping build for non-target branch: $VERCEL_GIT_COMMIT_REF\" && exit 1;; esac; else echo \"Local build detected, proceeding\"; fi",
    "build:dev": "dotenv -e .env.development next build",
    "build:staging": "dotenv -e .env.staging next build",
    "build:prod": "dotenv -e .env.production next build",
    "build:fast": "NEXT_COMPILE=1 next build",
    "build:full": "next build && node scripts/prepare-storybook.js && npm run build-storybook",
    "analyze": "ANALYZE=true npm run build",
    "bundle:check": "echo 'Bundle analysis available via npm run analyze'",
    "start": "next start",
    "start:staging": "dotenv -e .env.staging next start",
    "start:prod": "dotenv -e .env.production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fast": "next lint --max-warnings 0 --format compact",
    "type-check": "tsc --noEmit --incremental",
    "type-check:fast": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:fast": "jest --passWithNoTests --silent --maxWorkers=4",
    "refresh-stats": "node scripts/refresh-stats.js refresh",
    "check-stats": "node scripts/refresh-stats.js status",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}' '*.{js,jsx,ts,tsx,json,md}' '.storybook/**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}' '*.{js,jsx,ts,tsx,json,md}' '.storybook/**/*.{js,jsx,ts,tsx,json}'",
    "cleanup": "npm run format && npm run lint:fix",
    "pre-push": "npm run type-check && npm run lint && npm run test:ci",
    "security:check": "./scripts/security-check.sh",
    "security:scan": "npm audit --audit-level=moderate && audit-ci --config audit-ci.json",
    "security:audit": "npm audit --production && audit-ci --config audit-ci.json --skip-dev",
    "security:audit-fix": "npm audit fix --production",
    "security:snyk": "npx snyk test",
    "security:snyk-monitor": "npx snyk monitor",
    "security:full": "npm run security:scan && npm run security:snyk",
    "sonar:scan": "sonar-scanner",
    "quality:check": "npm run lint && npm run type-check && npm run test:coverage",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "git-flow": "./scripts/git-flow-helpers.sh",
    "feature:start": "./scripts/git-flow-helpers.sh start-feature",
    "feature:finish": "./scripts/git-flow-helpers.sh finish-feature",
    "release:start": "./scripts/git-flow-helpers.sh start-release",
    "release:finish": "./scripts/git-flow-helpers.sh finish-release",
    "hotfix:start": "./scripts/git-flow-helpers.sh start-hotfix",
    "flow:status": "./scripts/git-flow-helpers.sh status",
    "workflow": "./scripts/workflow.sh",
    "new-feature": "./scripts/workflow.sh feature",
    "commit": "./scripts/workflow.sh commit",
    "create-pr": "./scripts/workflow.sh pr",
    "git-status": "./scripts/workflow.sh status",
    "git-cleanup": "./scripts/workflow.sh cleanup",
    "complete-flow": "./scripts/workflow.sh complete",
    "deploy:setup": "./scripts/setup-deployment.sh setup",
    "deploy:status": "./scripts/setup-deployment.sh status",
    "deploy:test": "./scripts/setup-deployment.sh test",
    "deploy:storybook": "npm run build-storybook && echo 'Storybook built - deploy via GitHub Actions or Netlify CLI'",
    "setup-secrets": "./scripts/setup-secrets.sh",
    "prepare": "husky",
    "env:check": "npx ts-node -e \"import { logEnvironmentInfo } from './src/lib/env-config'; logEnvironmentInfo();\"",
    "_comment_tiers": "=== ENVIRONMENT-BASED QUALITY TIERS ===",
    "tier1:dev": "npm run dev:check && npm run dev:test",
    "dev:check": "npm run lint:fast && npm run type-check:fast",
    "dev:test": "npm run test:fast",
    "tier2:ci": "npm run ci:check && npm run ci:deps",
    "ci:check": "npm run lint && npm run type-check && npm run test:ci && npm run security:scan",
    "ci:deps": "npm run analyze:deps && npm run bundle:check",
    "tier3:staging": "npm run staging:test",
    "staging:test": "npm run test:e2e && npm run test:visual && npm run test:lighthouse && npm run test:a11y",
    "test:a11y": "jest --testNamePattern=\"accessibility\" --testTimeout=30000",
    "test:e2e": "echo 'E2E tests with Playwright - to be implemented'",
    "test:visual": "echo 'Visual regression tests - to be implemented'",
    "test:lighthouse": "echo 'Lighthouse performance tests - to be implemented'",
    "tier4:prod": "npm run prod:validate",
    "prod:validate": "npm run security:audit && npm run security:snyk && npm run analyze:bundle && echo 'Production validation completed'",
    "_comment_tools": "=== ANALYSIS & MONITORING TOOLS ===",
    "analyze:deps": "madge --circular --extensions ts,tsx src/",
    "analyze:deps-graph": "madge --image deps.svg src/",
    "analyze:bundle": "npm run analyze",
    "analyze:duplicates": "jscpd --min-lines 10 --min-tokens 70 --format 'typescript,javascript' --blame src/",
    "analyze:security": "npm run security:full",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "_comment_meta": "=== ENVIRONMENT-AWARE META COMMANDS ===",
    "check": "npx ts-node -e \"import { getQualityChecks } from './src/lib/env-config'; const checks = getQualityChecks(); console.log('Running:', checks.join(' && npm run ')); require('child_process').execSync('npm run ' + checks.join(' && npm run '), {stdio: 'inherit'});\"",
    "validate": "npm run check"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@hookform/resolvers": "^3.3.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^5.84.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.294.0",
    "next": "^15.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.47.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "^9.1.1",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "audit-ci": "^6.6.1",
    "autoprefixer": "^10.4.0",
    "buffer": "^6.0.3",
    "critters": "^0.0.23",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-storybook": "^9.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-junit": "^16.0.0",
    "jscpd": "^4.0.5",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.1.4",
    "madge": "^6.1.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "process": "^0.11.10",
    "semantic-release": "^24.2.7",
    "snyk": "^1.1291.0",
    "sonarqube-scanner": "^4.3.0",
    "storybook": "^9.1.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
